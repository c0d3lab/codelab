public class WeatherForecast{
	
	private String skies = "";
	private int high = 0;
	private int low = 0;
	
	public void setSkies(String a){
		skies = a;
	}
	
	public void setHigh(int a){
		high = a;
	}
	
	public void setLow(int a){
		low = a;
	}
	
	public String getSkies(){
		return skies;
	}
	
	public int getHigh(){
		return high;
	}
	
	public int getLow(){
		return low;
	}
	
}
